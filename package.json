{
  "scripts": {
    "i18nExport": "bundle exec rake i18n:js:export",
    "test": "jest",
    "lint": "eslint --ext .js,.jsx",
    "lint:all": "yarn run lint .",
    "lint:react": "yarn run lint {app,spec}/javascript/"
  },
  "jest": {
    "roots": [
      "<rootDir>/spec/javascript"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/spec/javascript/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "/spec/javascript/.*\\.test\\.jsx?$",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "resolutions_README": "Force babel-jest to use v23 as a workaround until we can upgrade to Babel 7. https://github.com/facebook/jest/issues/8230",
  "resolutions": {
    "babel-jest": "23.6.0"
  },
  "dependencies": {
    "@rails/webpacker": "3.5",
    "babel-preset-react": "^6.24.1",
    "jquery": "3.3.1",
    "lodash": "4.17.11",
    "mobx": "5.9.4",
    "mobx-react": "5.4.3",
    "prop-types": "^15.6.1",
    "query-string": "6.3.0",
    "react": "16.8.4",
    "react-bootstrap": "1.0.0-beta.6",
    "react-dom": "16.8.4",
    "react-select2-wrapper": "sassafrastech/react-select2-wrapper#2019-03-08-select2Full",
    "react_ujs": "^2.4.4"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-runtime": "6.23.0",
    "coffeelint": "2.1.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.11.2",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-cooperka": "1.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "i18n-js": "3.2.1",
    "jest": "24.7.1",
    "webpack-dev-server": "2.11.2"
  }
