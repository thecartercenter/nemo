{
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "i18n:export": "bundle exec rake i18n:js:export",
    "test": "jest",
    "lint:js": "eslint --ext .js,.jsx",
    "lint:react": "yarn run lint:js app/javascript/",
    "lint:backbone": "yarn run lint:js app/assets/javascripts/ --ignore-pattern app/assets/javascripts/legacy/",
    "lint:legacy": "yarn run lint:js app/assets/javascripts/legacy/",
    "lint:scss": "stylelint app/assets/stylesheets/",
    "sentry:setup": "echo \"[defaults]\nproject=nemo\norg=sassafras\" > ~/.sentryclirc && sentry-cli login",
    "sentry:tag": "if [[ ! -f ~/.sentryclirc ]]; then echo \"Please run sentry:setup first.\"; exit; fi; echo \"Setting v`cat VERSION` on Sentry to current HEAD...\"; sentry-cli releases set-commits \"nemo@`cat VERSION`\" --auto"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/app/javascript/jest.setup.js"
    ],
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>/app/javascript"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/app/javascript/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "/app/javascript/.*\\.test\\.jsx?$",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/app/javascript/__mocks__/style-mock.js",
      "^cheerio/lib/utils$": "cheerio/dist/commonjs/utils"
    }
  },
  "resolutions": {
    "@xmldom/xmldom": "0.7.7"
  },
  "babel": {
    "presets": [
      "./node_modules/shakapacker/package/babel/preset.js",
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "@babel/core": "7",
    "postcss": "^8.5.6",
    "@babel/helper-string-parser": "7.27.1",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.28.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7",
    "@babel/preset-env": "7",
    "@babel/preset-react": "7.27.1",
    "@babel/runtime": "7",
    "@sentry/react": "6.19.7",
    "@sentry/tracing": "6.19.7",
    "@types/babel__core": "7",
    "@types/webpack": "5",
    "babel-loader": "10",
    "cheerio": "^1.0.0-rc.3",
    "compression-webpack-plugin": "11",
    "core-js": "3.45.1",
    "css-loader": "^6.11.0",
    "enketo-core": "sassafrastech/enketo#enketo-core-8.3.1-fixGetters2",
    "ignore-loader": "0.1.2",
    "jquery": "3.6.0",
    "lodash": "4.17.21",
    "mini-css-extract-plugin": "^2.9.0",
    "mobx": "^5.15.7",
    "mobx-react": "5.4.3",
    "moment": "2.30.1",
    "prop-types": "15.8.0",
    "query-string": "6.14.1",
    "react": "16.x.x",
    "react-bootstrap": "1.6.8",
    "react-dates": "^21.8.0",
    "react-select2-wrapper": "sassafrastech/react-select2-wrapper#2019-03-08-select2Full",
    "react-with-direction": "^1.3.1",
    "react_ujs": "2.7.1",
    "regenerator-runtime": "0.14.0",
    "shakapacker": "8.4.0",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "5",
    "webpack": "5",
    "webpack-assets-manifest": "6",
    "webpack-merge": "6",
    "webpack-subresource-integrity": "5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.18.9",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.7",
    "@sentry/cli": "1.71.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.4.5",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-cooperka": "1.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "i18n-js": "3.8.0",
    "jest": "27.4.5",
    "react-dom": "16.13.0",
    "react-refresh": "0.14.0",
    "stylelint": "15.10.1",
    "stylelint-config-sass-guidelines": "7.0.0",
    "webpack-cli": "6",
    "webpack-dev-server": "5"
  },
  "name": "app",
  "private": true,
  "version": "0.1.0",
  "browserslist": [
    "defaults"
  ]
}
